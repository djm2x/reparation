{
  "name": "depannage",
  "version": "0.0.0",
  "scripts": {
    "start": "node ./program.js",
    "serve": "node ./publish-folder/program.js",
    "ssr": "ng run depannage:server:production && ng build --prod",
    "watch:serve": "ts-node ./program.ts",
    "watch": "nodemon -e ts -w ./ -x npm run watch:serve",
    "dev": "ts-node-dev --respawn --transpileOnly ./app/app.ts",
    "prod": "tsc && node ./dist/index.js",
    "sass": "npm rebuild node-sass",
    "compile_@agm_core": "babel node_modules/@agm/core -d node_modules/@agm/core --presets @babel/preset-env",
    "postinstall": "npm run compile_@agm_core",
    "cs": "tsc -p tsconfig.server.json",
    "bw": "ng build --extractCss --watch",
    "wp": "webpack --config webpack.config.js --progress --colors",
    "go": "git init && git remote add origin https://github.com/djm2x/reparation.git",
    "git": "git add . && git commit -m '1' && git push -u origin master",
    "log": "heroku logs --source app --app alaoui-reparation --tail"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.6",
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "~7.3.7",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "~7.2.0",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/platform-server": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@kolkov/angular-editor": "^0.14.5",
    "@nguniversal/common": "^7.1.1",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "0.0.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "hammerjs": "^2.0.8",
    "ngx-cacheable": "^1.2.2",
    "nodejs-nodemailer-outlook": "^1.1.0",
    "nodemailer": "^6.2.1",
    "nodemon": "^1.19.1",
    "preboot": "^7.0.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.7.7",
    "rxjs": "~6.3.3",
    "sqlite3": "^4.0.9",
    "tslib": "^1.9.0",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.17",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.16.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/nodemailer": "^6.2.0",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "webpack-cli": "^3.1.0"
  }
}
